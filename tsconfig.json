{
  "extends": "./packages/tsconfig/server.json",
  "compilerOptions": {
    "esModuleInterop": true,
    "outDir": "dist",
    "rootDir": "src",
    "baseUrl": ".",
    "paths": {
      "@src/*": ["src/*"]
    },
    "typeRoots": ["src/types"]
  },
  "include": ["src"],
  "exclude": ["node_modules"],
  "noEmit": false,
  "noEmitOnError": false,
}
